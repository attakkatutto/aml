<config>
    /*NETWORK CONFIG*/
    <numberAgentMessage>100</numberAgentMessage>
    <numberOfEntity>3000</numberOfEntity>
    <maxEdgesPerEntity>7</maxEdgesPerEntity>
    <numberPartners>5</numberPartners>
    <numberParents>4</numberParents>
    <numberDummies>3</numberDummies>
    <laundererPercentage>5</laundererPercentage>
    <guiEnabled>false</guiEnabled>
    /*HONEST*/
    <employeeMeanHonest>1</employeeMeanHonest>
    <employeeStdDevHonest>0.5</employeeStdDevHonest>
    <freelanceMeanHonest>5</freelanceMeanHonest>
    <freelanceStdDevHonest>1</freelanceStdDevHonest>
    <smallCompanyMeanHonest>80</smallCompanyMeanHonest>
    <smallCompanyStdDevHonest>10</smallCompanyStdDevHonest>
    <bigCompanyMeanHonest>300</bigCompanyMeanHonest>
    <bigCompanyStdDevHonest>20</bigCompanyStdDevHonest>
    /*LAUNDERER*/
    <employeeMeanLaunderer>3</employeeMeanLaunderer>
    <employeeStdDevLaunderer>0.3</employeeStdDevLaunderer>
    <freelanceMeanLaunderer>8</freelanceMeanLaunderer>
    <freelanceStdDevLaunderer>0.7</freelanceStdDevLaunderer>
    <smallCompanyMeanLaunderer>90</smallCompanyMeanLaunderer>
    <smallCompanyStdDevLaunderer>7</smallCompanyStdDevLaunderer>
    <bigCompanyMeanLaunderer>400</bigCompanyMeanLaunderer>
    <bigCompanyStdDevLaunderer>12</bigCompanyStdDevLaunderer>
    /*YEARS NUMBER*/ 
    <yearsNumber>5</yearsNumber>
    /*WRITE DATA 1=FILE 2=DB 3=ALL*/ 
    <persistenceMode>1</persistenceMode>
    /*FILE*/
    <fileNameTransaction>SYNTHETIC_%s.csv</fileNameTransaction>
    <fileNameEntity>ENTITIES_%s.csv</fileNameEntity>
    /*DATABASE*/
    <dataBaseUsername>sa</dataBaseUsername>
    <dataBasePassword>password</dataBasePassword>
    <dataBaseConnection>jdbc:derby://localhost:1527/synthetic</dataBaseConnection>
    <dataBaseDriver>org.apache.derby.jdbc.ClientDriver</dataBaseDriver>
</config>
